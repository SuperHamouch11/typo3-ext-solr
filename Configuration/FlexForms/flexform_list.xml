<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Search</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<targetPage>
						<TCEforms>
							<label>Target Page</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</targetPage>
					<ignorePageBrowser>
						<TCEforms>
							<label>Ignore Page Browser</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</ignorePageBrowser>
				</el>
			</ROOT>
		</sDEF>
		<sQuery>
			<ROOT>
				<TCEforms>
					<sheetTitle>Query</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<emptyQuery>
						<TCEforms>
							<label>Allow empty Query</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</emptyQuery>
					<filter>
						<TCEforms>
							<exclude>1</exclude>
							<label>Filter</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</filter>
					<sortBy>
						<TCEforms>
							<exclude>1</exclude>
							<label>Sorting</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</sortBy>
				</el>
			</ROOT>
		</sQuery>
		<sOptions>
			<ROOT>
				<TCEforms>
					<sheetTitle>Options</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<templateFile>
						<TCEforms>
							<exclude>1</exclude>
							<label>Template</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</templateFile>
				</el>
			</ROOT>
		</sOptions>
	</sheets>
</T3DataStructure>